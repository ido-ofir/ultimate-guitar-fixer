{
    "manifest_version": 2,
  
    "name": "Ultimate-guitar-fixer",
    "description": "Easily view and operate ultimate-guitar.com.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }